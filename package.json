{
  "name": "video-creator-portfolio",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test-jest":"jest --detectOpenHandles",
    "start-dev": "concurrently --kill-others \"npm run tw:build\" \"webpack-dev-server\"",
    "css-dev": "TAILWIND_MODE=watch postcss css/styles.css -o src/styles.css -w --verbose",
    "css-prod": "NODE_ENV=production postcss css/styles.css -o src/styles.css",
    "build": "export NODE_ENV=production && webpack",
    "dev": "npx webpack -w --config webpack.config.js --mode development",
    "prod": "npx webpack -w --config webpack.config.js --env production",
    "watch": "watch `npm run build` ./dist",
    "dev-backend": "nodemon backend/server.js",
    "tw:build": "npx tailwindcss -i ./frontend/src/css/tailwind.css -o ./frontend/dist/css/output.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
