    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.15",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "google-auth-library": "^7.11.0",
